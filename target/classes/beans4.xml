<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
                http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="address" class="com.obj.Employee.Address">

		<constructor-arg value="Tirupati"></constructor-arg>
		<constructor-arg value="AP"></constructor-arg>
		<constructor-arg value="India"></constructor-arg>

	</bean>
	<bean id="emp" class="com.obj.Employee.Employee">
		<constructor-arg value="19" type="int"></constructor-arg>
		<constructor-arg value="Sivaji kadapala"></constructor-arg>
		<constructor-arg>
			<ref bean="address" />
		</constructor-arg>
	</bean>

</beans>